load "plot-defaults-ps.gpi"

set xlabel "Action"
set ylabel "Number of rules"
set output "../build/plot-action-distribution.ps"

# Put the legend at the top left, Left justified text.
set key top left Left reverse
# Let the X axis continue to the next round number for this graph.
#set autoscale
set title "Distribution of rules per action"

set xtics rotate
plot "../build/action-distribution.txt" using 1:3:xticlabels(2) with lines \
    title "Distribution of rules per action"
