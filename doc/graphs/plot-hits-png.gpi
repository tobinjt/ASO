load "plot-defaults-png.gpi"

set xlabel "Rule"
set ylabel "Number of hits"
set output "../build/plot-hits.png"

# Put the legend at the top left, Left justified text.
set key top left Left reverse
# Let the X axis continue to the next round number for this graph (140; the 
# last point is 139).
set autoscale
set yrange [0:9e6]
set title "Number of hits"

plot "../stats-hits.txt" using 1:2 title "Number of hits"
