load "plot-defaults-pdf.gpi"

set xlabel "Log file"
set ylabel "Execution time of normal and reverse ordering \n relative to shuffled ordering"
set output "../build/plot-normal-and-reverse-relative-to-shuffle.pdf"
# Add a % sign on the y axis
set format y "% g%%"

plot 100 with lines title "Shuffle", \
    "../build/stats-normal-shuffle-reverse-factor.txt" using 1 with lines title "Normal", \
    "../build/stats-normal-shuffle-reverse-factor.txt" using 2 with lines title "Reverse"
