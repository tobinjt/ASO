load "plot-defaults-ps.gpi"

set xlabel "Log file"
set ylabel "Percentage decrease in parsing time of\nperfect best oracle relative to optimal ordering"
set output "../build/graph-perfect-best-vs-optimal.ps"
# Add a % sign on the y axis
set format y "% g%%"
# Remove the horizontal lines.
set grid noy

plot "../build/stats-perfect-best-vs-optimal" using 1 with lines title "Perfect best oracle relative to optimal ordering"
