set xlabel "Program"
set ylabel "Number of matching rules"
set grid ytics

set terminal postscript monochrome eps size 5.4,3.3
set output "plot-log22-distribution-by-program.ps"
# Put the legend at the top left, Left justified text.
set key top left Left reverse

set xrange [0:7]
plot "stats-log22-distribution-by-program.txt" using 1:2:xticlabels(3) title "Number of matching rules"
