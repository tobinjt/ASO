load "plot-defaults-pdf.gpi"

set xlabel "Log file"
set ylabel "Percentage increase of shuffled and reverse over normal"
set output "../build/plot-normal-shuffle-reverse-factor.pdf"
# Add a % sign on the y axis
set format y "% g%%"

plot "../build/stats-normal-shuffle-reverse-factor.txt" using 1 with lines \
        title "Shuffle vs normal",                                         \
     "../build/stats-normal-shuffle-reverse-factor.txt" using 2 with lines \
        title "Reverse vs normal"
